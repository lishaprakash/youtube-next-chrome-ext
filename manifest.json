{
  "manifest_version": 2,
  "name": "Play Next Youtube Song",
  "version": "1.0",
  "description": "Play the next youtube suggested song",
  "homepage_url": "https://github.com/lishaprakash/youtube-next-chrome-ext", 
  "icons": { 
           "16":"icon16.png", 
           "48":"icon48.png", 
           "128":"icon128.png" 
        },
  "browser_action": {
  "default_icon": "icon48.png"
  },
  "background": {
  "scripts": ["background.js"]
  },
  "permissions": ["tabs","https://www.youtube.com/*"]
}
