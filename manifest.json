{
  "manifest_version": 2,
  "name": "Youtube Next Player",
  "version": "0.1",
  "icons": { 
           "16":"icon16.png", 
           "48":"icon48.png", 
           "128":"icon128.png" 
        },
  "browser_action": {
  "default_icon": "icon48.png"
  },
  "background": {
  "scripts": ["background.js"]
  },
  "permissions": ["tabs","https://www.youtube.com/*"]
}
